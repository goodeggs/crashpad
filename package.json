{
  "name": "crashpad",
  "version": "1.3.0",
  "description": "Express middleware for Boom errors",
  "author": "Good Eggs <open-source@goodeggs.com>",
  "contributors": [
    "Max Edmands <me@maxedmands.com>"
  ],
  "engines": {
    "node": "^14.16.0",
    "yarn": "1.9.4"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "local-modules"
    ]
  },
  "license": "MIT",
  "keywords": [
    "express boom"
  ],
  "main": "src/index.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/goodeggs/crashpad.git"
  },
  "homepage": "https://github.com/goodeggs/crashpad",
  "bugs": "https://github.com/goodeggs/crashpad/issues",
  "dependencies": {
    "@hapi/boom": "^9.1.4"
  },
  "scripts": {
    "build": "yarn run clean && yarn run build:src && yarn run build:types",
    "build:src": "babel --extensions .ts,.js,.jsx,.tsx --source-maps --ignore 'src/*.test.ts' --out-dir dist/ src/",
    "build:types": "tsc --project tsconfig.declarations.json",
    "clean": "rm -rf dist",
    "lint": "eslint --ext js,jsx,ts,tsx --ignore-path .gitignore .",
    "test": "mocha --require @babel/polyfill --require .register.ts --extension ts,js,jsx,tsx"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-typescript": "^7.15.0",
    "@babel/register": "^7.15.3",
    "@goodeggs/toolkit": "^4.0.4",
    "@goodeggs/tsconfig": "^1.0.0",
    "@types/chai": "^4.2.22",
    "@types/express": "^4.17.13",
    "@types/mocha": "^9.0.0",
    "@types/request": "^2.48.7",
    "babel-plugin-local-modules": "^1.0.4",
    "chai": "^4.3.4",
    "express": "^4.17.1",
    "mocha": "^9.1.2",
    "request": "^2.88.2",
    "typescript": "^4.4.3"
  }
}
